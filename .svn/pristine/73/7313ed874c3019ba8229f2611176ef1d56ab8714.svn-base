package cn.ucai.entity;
/**
 * 窗口实体类
 * @author Administrator
 *
 */
public class Window {
	private Integer id;
	private int no;//窗口号
	private Integer b_id;//business的外键
	
	private int disabled;//是否禁用：0：未禁用 1：禁用
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public Integer getB_id() {
		return b_id;
	}
	public void setB_id(Integer b_id) {
		this.b_id = b_id;
	}
	
	public int getDisabled() {
		return disabled;
	}
	public void setDisabled(int disabled) {
		this.disabled = disabled;
	}
	@Override
	public String toString() {
		return "Window [id=" + id + ", no=" + no + ", b_id=" + b_id + ", disabled=" + disabled + "]";
	}
	
	
	
}
